<div class="row profile-timeline">
  <div class="col-md-6">
    <div class="timeline-section timeline-section-about">
      <h3 class="title">Sobre</h2>
      <div class="content">
      {{user.biography}}
      </div>
    </div>
    <div class="timeline-section timeline-section-relatos timeline-collapse blue">
      <h3 class="title"><i class="IR sprt-chevron-up"></i> Relatos</h2>
      <div class="content">
        <ul class="relatos-list">
		      {{#each relato in relatosProxy}}
		      	<li><i class="IR sprt-relato-1"></i>{{#link-to 'relato' relato.id}}{{relato.titulo}}{{/link-to}}</li>
		      {{/each}}          
        </ul>
      </div>
      {{#link-to 'user.relatos' class="btn btn-link link-more"}}Ver todos{{/link-to}}
    </div>
    <div class="timeline-section timeline-section-comunidades blue timeline-collapse">
      <h3 class="title"><i class="IR sprt-chevron-up"></i> Comunidades</h2>
      <div class="content">
        <div class="carousel slide carousel-multiple" id="carousel-comunidades"  data-ride="carousel">
                <div class="carousel-inner">
                  <div class="item active">
                    <div class="col-sm-4">
                    <a href="#"><img src="http://lorempixel.com/142/201420/sports" class="img-responsive img-circle">Boas Vindas!</a></div>
                  </div>
                  <div class="item">
                    <div class="col-sm-4">
                    <a href="#"><img src="http://lorempixel.com/142/142/" class="img-responsive img-circle">Histórias do Cotidiano na Atenção Básica</a></div>
                  </div>
                  <div class="item">
                    <div class="col-sm-4">
                    <a href="#"><img src="http://lorempixel.com/142/142/sports" class="img-responsive img-circle">Educação e Saúde Bucal</a></div>
                  </div>
